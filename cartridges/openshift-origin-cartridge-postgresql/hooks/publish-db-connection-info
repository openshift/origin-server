#!/bin/bash

postgresql_url="postgresql://$OPENSHIFT_POSTGRESQL_DB_USERNAME:$OPENSHIFT_POSTGRESQL_DB_PASSWORD@$OPENSHIFT_GEAR_DNS:$OPENSHIFT_POSTGRESQL_DB_PROXY_PORT/"

echo -n "OPENSHIFT_POSTGRESQL_DB_USERNAME=$OPENSHIFT_POSTGRESQL_DB_USERNAME; "
echo -n "OPENSHIFT_POSTGRESQL_DB_PASSWORD=$OPENSHIFT_POSTGRESQL_DB_PASSWORD; "
echo -n "OPENSHIFT_POSTGRESQL_DB_HOST=$OPENSHIFT_GEAR_DNS; "
echo -n "OPENSHIFT_POSTGRESQL_DB_PORT=$OPENSHIFT_POSTGRESQL_DB_PROXY_PORT; "
echo -n "OPENSHIFT_POSTGRESQL_DB_URL=$postgresql_url; "
