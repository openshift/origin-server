#!/bin/bash
mongodb_url="mongodb://$OPENSHIFT_MONGODB_DB_USERNAME:$OPENSHIFT_MONGODB_DB_PASSWORD@$OPENSHIFT_GEAR_DNS:$OPENSHIFT_MONGODB_DB_PROXY_PORT/"

echo -n "OPENSHIFT_MONGODB_DB_USERNAME=$OPENSHIFT_MONGODB_DB_USERNAME; "
echo -n "OPENSHIFT_MONGODB_DB_PASSWORD=$OPENSHIFT_MONGODB_DB_PASSWORD; "
echo -n "OPENSHIFT_MONGODB_DB_HOST=$OPENSHIFT_GEAR_DNS; "
echo -n "OPENSHIFT_MONGODB_DB_PORT=$OPENSHIFT_MONGODB_DB_PROXY_PORT"
echo -n "OPENSHIFT_MONGODB_DB_URL=$mongodb_url; "
