#!/bin/bash
mongodb_url="mongodb://$OPENSHIFT_MONGODB_DB_USERNAME:$OPENSHIFT_MONGODB_DB_PASSWORD@$OPENSHIFT_GEAR_DNS:$OPENSHIFT_MONGODB_DB_PROXY_PORT/"

echo "OPENSHIFT_MONGODB_DB_GEAR_UUID=$OPENSHIFT_GEAR_UUID"
echo "OPENSHIFT_MONGODB_DB_GEAR_DNS=$OPENSHIFT_GEAR_DNS"
echo "OPENSHIFT_MONGODB_DB_USERNAME=$OPENSHIFT_MONGODB_DB_USERNAME"
echo "OPENSHIFT_MONGODB_DB_PASSWORD=$OPENSHIFT_MONGODB_DB_PASSWORD"
echo "OPENSHIFT_MONGODB_DB_HOST=$OPENSHIFT_GEAR_DNS"
echo "OPENSHIFT_MONGODB_DB_PORT=$OPENSHIFT_MONGODB_DB_PROXY_PORT"
echo "OPENSHIFT_MONGODB_DB_URL=$mongodb_url"
