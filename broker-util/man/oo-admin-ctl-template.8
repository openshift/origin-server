\fBoo-admin-ctl-template
\fB
.SH SYNOPSIS
.nf
.fam C
\fBoo-admin-ctl-template\fP [\fB-h\fP|\fB--help\fP] [\fB-u\fP|\fB--uuid\fP \fItemplate_uuid\fP] 
[\fB-n\fP|\fB--named\fP \fItemplate_name\fP] [\fB-a\fP|\fB--all\fP] [\fB-c\fP|\fB--command\fP \fIcmd\fP] 
